{erl_opts, [debug_info]}.

{plugins,
 [ {grpc_plugin, {git, "https://github.com/HJianBo/grpcbox_plugin", {branch, "develop"}}}
 ]}.

{deps,
 [ {gpb, "~> 4.11"}
 , {gproc, "0.8.0"}
 , {cowboy, "2.8.0"}
 , {gun, "1.3.3"}
 ]}.

{grpc,
 [{type, client},
  {protos, ["test/"]},
  {gpb_opts, [{module_name_prefix, "emqx_"},
              {module_name_suffix, "_pb"}]}
]}.
